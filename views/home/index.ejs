<% include ../partials/header %>
<div class="row" >
    <div id="cbox">
        <section class="cboxPf">
            <div class="row" >
                <div class="col s12 center-align" >
                    <header>
                        <h1 class="flow-text">TCC</h1>
                        <h4 class="flow-text">Bem Vindo!</h4>
                    </header>
                </div>
            </div>
            <div class="row">
                <form class="col s12 center-align" action="/entrar" method="post">
                    <div class="row">
                        <div class="input-field col s10 offset-s1">
                            <input type="text" name="usuario[email]" id="email" >
                            <label for="email">Email:</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s10  offset-s1">                   
                            <input type="password" name="usuario[password]" id="password" >
                            <label for="password">Senha:</label>
                        </div>
                    </div>
                    <div class="row" >
                        <div class="col s12" >
                            <button class="btn waves-effect waves-light blue lighten-2" type="submit" name="action">Entrar
                                <i class="material-icons right">send</i>
                            </button>
                        </div>
                    </div>
                </form>
            </div>
        </section>
    </div>
</div>

<% include ../partials/footer %>
